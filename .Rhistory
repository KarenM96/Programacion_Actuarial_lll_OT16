z<-5
caminata <-vector("numeric")
while(z>=3 && z<=10){
print(z)
caminata<-c(caminata,z)
moneda <- rbinom(1,1,0.5)
if(moneda==1){ #caminata aleatoria
z<-z+1
}else{
z<-z-1
}
}
